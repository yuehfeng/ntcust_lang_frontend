\"10\"\r\n                max-rows=\"6\"\r\n                ></b-form-textarea>\r\n            <b-row class=\"my-2\">\r\n                <b-col md=\"2\">\r\n                    <label for=\"\">消息類型</label>\r\n                </b-col>\r\n                <b-col md=\"10\">\r\n                    <b-form-select v-model=\"creating.notification.id\" :options=\"options\"></b-form-select>\r\n                </b-col>\r\n                <b-col md=\"12 text-center\">\r\n                    <b-button variant=\"success m-2\" @click=\"createData\">創建</b-button>\r\n                </b-col>\r\n            </b-row>\r\n        </b-card>\r\n    </b-collapse>\r\n</template>\r\n\r\n<script>\r\nimport axios from 'axios';\r\n\r\nexport default {\r\n    name: 'App',\r\n    data(){\r\n        return {\r\n            options: [\r\n                { value: 1, text: '公告' },\r\n                { value: 2, text: '研習' },\r\n                { value: 3, text: '活動' },\r\n                { value: 4, text: '競賽' },\r\n                { value: 5, text: '徵才' }\r\n            ],\r\n            creating: {\r\n                data: [],\r\n                notification: {\r\n                    id: 1\r\n                }\r\n            },\r\n        }\r\n    },\r\n    created(){\r\n        \r\n    },\r\n    methods: \r\n    {\r\n        createData(){\r\n            axios({\r\n                method: 'post',\r\n                url: 'http://localhost/ntcust_lang_backend/public/api/news/create/',\r\n                data: {\r\n                    \"title\": this.creating.data.title,\r\n                    \"context\": this.creating.data.context,\r\n                    \"reports\": \"1\",\r\n                    \"type\": Number(this.creating.notification.id)\r\n                },\r\n                dataType: 'json',\r\n                headers: {\r\n                    \"Content-Type\": \"application/json\",\r\n                    \"Accept\": \"application/json\",\r\n                    \"Authorization\": 'Bearer ' + localStorage.getItem('API_TOKEN'),\r\n                }\r\n            })\r\n                .then(response => {\r\n                    console.log(response.data);\r\n                    this.response.value = true;\r\n                    this.response.status = true;\r\n                })\r\n                .catch( (error) => {\r\n                    console.log(error);\r\n                    this.response.value = true;\r\n                    this.response.status = false;\r\n                })\r\n        }\r\n    }\r\n}\r\n</script>\r\n"]}]}