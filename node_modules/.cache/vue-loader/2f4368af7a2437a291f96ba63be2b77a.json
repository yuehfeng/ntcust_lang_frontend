n    name: 'App',\r\n    components: {\r\n        ManageBannerCreate\r\n    },\r\n    data(){\r\n        return {\r\n            banners: []\r\n        }\r\n    },\r\n    created(){\r\n        this.moment = moment;\r\n        axios\r\n            .get('http://localhost/ntcust_lang_backend/public/api/banner')\r\n            .then(response => {\r\n                this.banners = response.data.data;\r\n            })\r\n            .catch(error => {\r\n                console.error(error)\r\n            });\r\n    },\r\n    computed:{\r\n    },\r\n    methods: \r\n    {\r\n    }\r\n}\r\n</script>\r\n"]}]}