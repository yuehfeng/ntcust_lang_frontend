t>\r\n    <b-alert v-model=\"response.error\" variant=\"danger\" dismissible>\r\n        {{ response.feedback }}\r\n    </b-alert>\r\n    <b-collapse id=\"collapse-1\" class=\"m-2\">\r\n        <b-card>\r\n            <b-row class=\"my-2\">\r\n                <b-col md=\"2\">\r\n                    <label for=\"\">消息標題</label>\r\n                </b-col>\r\n                <b-col md=\"10\">\r\n                    <b-form-input v-model=\"creating.data.title\" placeholder=\"請填入消息標題\"></b-form-input>\r\n                </b-col>\r\n            </b-row>\r\n\r\n            <b-row class=\"my-2\">\r\n                <b-col md=\"2\">\r\n                    <label for=\"\">內文</label>\r\n                </b-col>\r\n                <b-col md=\"10\">\r\n                    <ckeditor v-model=\"creating.data.context\" :config=\"editorConfig\" :editor=\"editor\"></ckeditor>\r\n                </b-col>\r\n            </b-row>\r\n            <b-row class=\"my-2\">\r\n                <b-col md=\"2\">\r\n                    <label for=\"\">消息類型</label>\r\n                </b-col>\r\n                <b-col md=\"10\">\r\n                    <b-form-select v-model=\"creating.notification.id\" :options=\"options\"></b-form-select>\r\n                </b-col>\r\n                <b-col md=\"12 text-center\">\r\n                    <b-button variant=\"success m-2\" @click=\"createData\">創建</b-button>\r\n                </b-col>\r\n            </b-row>\r\n        </b-card>\r\n    </b-collapse>\r\n</template>\r\n\r\n<script>\r\nimport { NewsServer } from \"../../providers/http-server.js\";\r\n\r\nlet ClassicEditor \r\nif (process.browser) {\r\n  ClassicEditor = require('@ckeditor/ckeditor5-build-classic')\r\n}\r\n\r\nClassicEditor\r\n    .create( document.querySelector( '#editor' ), {\r\n        language: 'zh-tw'\r\n    } )\r\n    .then( editor => {\r\n        console.log( editor );\r\n    } )\r\n    .catch( error => {\r\n        console.error( error );\r\n    } );\r\n\r\nexport default {\r\n    name: 'App',\r\n    data(){\r\n        return {\r\n            options: [\r\n                { value: 1, text: '公告' },\r\n                { value: 2, text: '研習' },\r\n                { value: 3, text: '活動' },\r\n                { value: 4, text: '競賽' },\r\n                { value: 5, text: '徵才' }\r\n            ],\r\n            creating: {\r\n                data: [],\r\n                notification: {\r\n                    id: 1\r\n                }\r\n            },\r\n            response: {\r\n                status: false,\r\n                feedback: false,\r\n                error: false,\r\n                code: 200\r\n            },\r\n            editor: ClassicEditor,\r\n            editorConfig: {\r\n                toolbar: {\r\n                },\r\n            }\r\n        }\r\n    },\r\n    methods: \r\n    {\r\n        createData(){\r\n            let data = {\r\n                \"title\": this.creating.data.title,\r\n                \"context\": this.creating.data.context,\r\n                \"type\": Number(this.creating.notification.id),\r\n            };\r\n            NewsServer.create(data).then(() => {\r\n                this.response.error = false;\r\n                this.response.status = true;\r\n                this.$router.go(0);\r\n            }).catch(()=> {\r\n                this.response.error = true;\r\n                this.response.status = true;\r\n            });\r\n        }\r\n    }\r\n}\r\n</script>\r\n"]}]}