            })\r\n            .catch(error => {\r\n                console.error(error)\r\n            });\r\n    },\r\n    computed:{\r\n    },\r\n    methods: \r\n    {\r\n        edit(index)\r\n        {\r\n            this.editinglinks = this.links[index];\r\n        },\r\n        checkFormValidity() {\r\n            const valid = this.$refs.form.checkValidity()\r\n            this.nameState = valid\r\n            return valid\r\n        },\r\n        createLink() {\r\n            axios({\r\n                method: 'post',\r\n                url: 'http://localhost/ntcust_lang_backend/public/api/link/create',\r\n                data: {\r\n                    name: this.creatinglinks.name,\r\n                    url: this.creatinglinks.url,\r\n                    type: this.creatinglinks.type,\r\n                    download: this.creatinglinks.download\r\n                },\r\n                headers: {\r\n                    \"Content-Type\": \"application/json\",\r\n                    \"Accept\": \"application/json\",\r\n                    \"Authorization\": 'Bearer ' + localStorage.getItem('API_TOKEN'),\r\n                }\r\n            })\r\n            .then( () => {\r\n                // console.log(response.data);\r\n            })\r\n            .catch( (error) => {\r\n                console.log(Promise.reject(error));\r\n            });\r\n        },\r\n        updateLink() {\r\n            axios({\r\n                method: 'post',\r\n                url: 'http://localhost/ntcust_lang_backend/public/api/link/update/' + this.editinglinks.id,\r\n                data: this.editinglinks,\r\n                headers: {\r\n                    \"Content-Type\": \"application/json\",\r\n                    \"Accept\": \"application/json\",\r\n                    \"Authorization\": 'Bearer ' + localStorage.getItem('API_TOKEN'),\r\n                }\r\n            })\r\n            .then( () => {\r\n                // console.log(response.data);\r\n            })\r\n            .catch( (error) => {\r\n                console.log(Promise.reject(error));\r\n            });\r\n        },\r\n    }\r\n}\r\n</script>\r\n"]}]}