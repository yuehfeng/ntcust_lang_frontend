    <div class=\"container\">\n        <h1>{{ album.name }}</h1>\n        <div class=\"text-end\">\n            <i>{{ moment(album.updated_at).format(\"YYYY-MM-DD HH:MM\") }}</i>\n        </div>\n        <div class=\"alert alert-light\" role=\"alert\">{{ album.description }}</div>\n        \n        <p>{{ album.context }}</p>\n\n        \n\n        <div class=\"text-center\">\n            <span>瀏覽次數: {{ album.views }}</span>\n            <span> | </span>\n            <span>發布者: {{ album.editor }}</span>\n        </div>\n        <b-row>\n            <b-col md=\"4\" v-for=\"(item, index) in photos\" :key=\"index\">\n                <b-img :src=\"'http://localhost/ntcust_lang_backend/storage/app/' + item.wpath\" fluid alt=\"Responsive image\"></b-img>\n            </b-col>\n        </b-row>\n        <div class=\"text-center\">\n            <button type=\"button\" class=\"btn btn-secondary my-3\" @click=\"historyPrev\">回上頁</button>\n        </div>\n    </div>\n</template>\n\n<script>\nimport moment from 'moment'\nimport axios from 'axios';\n\nexport default {\n    name: 'App',\n    data(){\n        return {\n            album: [],\n            photos: []\n        }\n    },\n    async created(){\n        this.moment = moment;\n        axios\n            .get('http://localhost/ntcust_lang_backend/public/api/album/' + this.$route.params.id)\n            .then(response => {\n                this.album = response.data.data;\n            })\n            .catch(error => {\n                console.error(error)\n            });\n        axios\n            .get('http://localhost/ntcust_lang_backend/public/api/file/filter/' + this.$route.params.id + '/3')\n            .then(response => {\n                this.photos = response.data.data;\n            })\n            .catch(error => {\n                console.error(error)\n            });\n    },\n    methods: {\n        historyPrev()\n        {\n            this.$router.go(-1);\n        }\n    }\n}\n</script>\n"]}]}