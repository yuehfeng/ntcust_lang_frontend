AAC,CAAC,CAAC,CAAC,CAAC,EAAE,EAAE,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC;gBAC3C;YACJ,CAAC;QACL;IACJ;AACJ","file":"C:/www-server/ntcust_lang_frontend/src/views/components/Manage-News.vue","sourceRoot":"","sourcesContent":["<template>\r\n    <ManageNewsCreate></ManageNewsCreate>\r\n\r\n                    <ckeditor></ckeditor>\r\n    <b-tab title=\"消息管理\" active>\r\n        <b-row>\r\n            <b-col md=\"4\">\r\n                <b-list-group v-for=\"(item,index) in news\" :key=\"index\">\r\n                    <b-list-group-item active v-if=\"index == editing.current\" @click=\"select(index)\">\r\n                        <b-badge>{{ item.id }}</b-badge>\r\n                        {{ item.title }}\r\n                    </b-list-group-item>\r\n                    <b-list-group-item v-if=\"index != editing.current\" @click=\"select(index)\">\r\n                        <b-badge>{{ item.id }}</b-badge>\r\n                        {{ item.title }}\r\n                    </b-list-group-item>\r\n                </b-list-group>\r\n                <div class=\"d-flex justify-content-center my-2\">\r\n                    <nav aria-label=\"Page navigation example\">\r\n                        <ul class=\"pagination justify-content-end\">\r\n                            <li class=\"page-item disabled\" v-if=\"page.current == 1\">\r\n                                <a @click=\"prevPage\" class=\"page-link\">Previous</a>\r\n                            </li>\r\n                            <li class=\"page-item\" v-if=\"page.current != 1\">\r\n                                <a @click=\"prevPage\" class=\"page-link\">Previous</a>\r\n                            </li>\r\n                            <li class=\"page-item\">\r\n                                <a class=\"page-link bg-primary text-light fw-bolder\">{{ page.current }}</a>\r\n                            </li>\r\n                            <li class=\"page-item disabled\" v-if=\"page.current == page.all\">\r\n                                <a @click=\"nextPage\" class=\"page-link\">Next</a>\r\n                            </li>\r\n                            <li class=\"page-item\" v-if=\"page.current != page.all\">\r\n                                <a @click=\"nextPage\" class=\"page-link\">Next</a>\r\n                            </li>\r\n                        </ul>\r\n                    </nav>\r\n                </div>\r\n            </b-col>\r\n            <b-col md=\"8\">\r\n                <b-alert v-model=\"response.status\" variant=\"success\" dismissible>\r\n                    操作成功\r\n                </b-alert>\r\n                <b-alert v-model=\"response.error\" variant=\"danger\" dismissible>\r\n                    {{ response.feedback }}\r\n                </b-alert>\r\n            \r\n                <b-row class=\"my-2\">\r\n                    <b-col md=\"2\">\r\n                        <label for=\"\">標題編號</label>\r\n                    </b-col>\r\n                    <b-col md=\"10\">\r\n                        <b-form-input v-model=\"editing.data.id\" placeholder=\"Enter your name\" disabled></b-form-input>\r\n                    </b-col>\r\n                </b-row>\r\n                <b-row class=\"my-2\">\r\n                    <b-col md=\"2\">\r\n                        <label for=\"\">消息標題</label>\r\n                    </b-col>\r\n                    <b-col md=\"10\">\r\n                        <b-form-input v-model=\"editing.data.title\" placeholder=\"Enter your name\"></b-form-input>\r\n                    </b-col>\r\n                </b-row>\r\n\r\n                <label for=\"\">內文</label>\r\n                <b-form-textarea\r\n                    id=\"textarea\"\r\n                    v-model=\"editing.data.context\"\r\n                    placeholder=\"Enter something...\"\r\n                    rows=\"10\"\r\n                    max-rows=\"6\"\r\n                    ></b-form-textarea>\r\n                <b-row class=\"my-2\">\r\n                    <b-col md=\"2\">\r\n                        <label for=\"\">消息類型</label>\r\n                    </b-col>\r\n                    <b-col md=\"10\">\r\n                        <b-form-select v-model=\"editing.notification.id\" :options=\"options\"></b-form-select>\r\n                    </b-col>\r\n                </b-row>\r\n                <b-row class=\"my-2\">\r\n                    <b-col md=\"2\">\r\n                        <label for=\"\">編輯者</label>\r\n                    </b-col>\r\n                    <b-col md=\"10\">\r\n                        <b-form-input disabled v-model=\"editing.data.editor\" placeholder=\"Enter your name\"></b-form-input>\r\n                    </b-col>\r\n                </b-row>\r\n                <b-row class=\"my-2\">\r\n                    <b-col md=\"2\">\r\n                        <label for=\"\">建立時間</label>\r\n                    </b-col>\r\n                    <b-col md=\"10\">\r\n                        {{ moment(editing.data.created_at).format(\"YYYY-MM-DD HH:MM\") }}\r\n                    </b-col>\r\n                </b-row>\r\n                <b-row class=\"my-2\">\r\n                    <b-col md=\"2\">\r\n                        <label for=\"\">編輯時間</label>\r\n                    </b-col>\r\n                    <b-col md=\"10\">\r\n                        {{ moment(editing.data.updated_at).format(\"YYYY-MM-DD HH:MM\") }}\r\n                    </b-col>\r\n                </b-row>\r\n                <b-row class=\"my-2\">\r\n                    <b-col md=\"2\">\r\n                        <label for=\"\">下架時間</label>\r\n                    </b-col>\r\n                    <b-col md=\"10\">\r\n                        {{ moment(editing.data.deleted_at).format(\"YYYY-MM-DD HH:MM\") }}\r\n                    </b-col>\r\n                </b-row>\r\n                <b-row>\r\n                    <b-col md=\"12 text-center\">\r\n                        <b-button variant=\"success m-2\" @click=\"updateData(editing.data.id)\">更新</b-button>\r\n                        <b-button variant=\"primary m-2\" @click=\"topData(editing.data.id)\">\r\n                            <span v-if=\"!editing.data.top\">置頂</span>\r\n                            <span v-if=\"editing.data.top\">取消置頂</span>\r\n                        </b-button>\r\n                        <b-button variant=\"warning m-2\" @click=\"softDelete(editing.data.id)\">\r\n                            <span v-if=\"editing.data.deleted_at == null\">下架</span>\r\n                            <span v-if=\"editing.data.deleted_at != null\">取消下架</span>\r\n                        </b-button>\r\n                        <b-button variant=\"danger m-2\" @click=\"deleteData(editing.data.id)\">刪除</b-button>\r\n                    </b-col>\r\n                </b-row>\r\n            </b-col>\r\n        </b-row>\r\n    </b-tab>\r\n</template>\r\n\r\n<script>\r\nimport moment from 'moment';\r\nimport ManageNewsCreate from './Manage-News-Create.vue';\r\nimport { NewsServer } from \"../../providers/http-server.js\";\r\n\r\nexport default {\r\n    name: 'App',\r\n    components: {\r\n        ManageNewsCreate\r\n    },\r\n    data(){\r\n        return {\r\n            options: [\r\n                { value: 1, text: '公告' },\r\n                { value: 2, text: '研習' },\r\n                { value: 3, text: '活動' },\r\n                { value: 4, text: '競賽' },\r\n                { value: 5, text: '徵才' }\r\n            ],\r\n            news: [],\r\n            editing: {\r\n                current: 0,\r\n                data: [],\r\n                notification: []\r\n            },\r\n            response: {\r\n                status: false,\r\n                error: false,\r\n                feedback: \"\",\r\n            },\r\n            page: {\r\n                current: 1,\r\n                all: 1\r\n            },\r\n            editorData: '<p>Content of the editor.</p>',\r\n        }\r\n    },\r\n    created(){\r\n        this.moment = moment;\r\n        NewsServer.readALL().then(\r\n            (response) => {\r\n                this.news = response.data.result;\r\n                this.editing.data = this.news[0];\r\n                this.editing.notification = this.news[0].notification;\r\n                this.page.current = response.data.pages.current;\r\n                this.page.all = response.data.pages.all;\r\n            }\r\n        );\r\n    },\r\n    methods: {\r\n        updateData(id){\r\n            let data = {\r\n                \"title\": this.editing.data.title,\r\n                \"context\": this.editing.data.context,\r\n                \"type\": Number(this.editing.notification.id),\r\n            };\r\n            NewsServer.update(id, data).then(() => {\r\n                this.response.error = false;\r\n                this.response.status = true;\r\n            }).catch(()=> {\r\n                this.response.error = true;\r\n                this.response.status = true;\r\n            })\r\n        },\r\n        softDelete(id){\r\n            NewsServer.softdelete(id, null).then(() => {\r\n                this.response.error = false;\r\n                this.response.status = true;\r\n            }).catch(()=> {\r\n                this.response.error = true;\r\n                this.response.status = true;\r\n            })\r\n        },\r\n        deleteData(id){\r\n            NewsServer.delete(id, null).then(() => {\r\n                this.response.error = false;\r\n                this.response.status = true;\r\n            }).catch(()=> {\r\n                this.response.error = true;\r\n                this.response.status = true;\r\n            });\r\n        },\r\n        topData(id){\r\n            NewsServer.top(id, null).then(() => {\r\n                this.response.error = false;\r\n                this.response.status = true;\r\n            }).catch(()=> {\r\n                this.response.error = true;\r\n                this.response.status = true;\r\n            });\r\n        },\r\n        select(num){\r\n            this.response.status = false;\r\n            this.editing.current = num;\r\n            this.editing.data = this.news[num];\r\n            this.editing.notification = this.news[num].notification;\r\n        },\r\n        prevPage() {\r\n            this.page.current -= 1;\r\n            NewsServer.readALL({\r\n                page: this.page.current\r\n            }).then(\r\n                (response) => {\r\n                    this.news = response.data.result;\r\n                    this.editing.data = this.news[0];\r\n                    this.editing.notification = this.news[0].notification;\r\n                    this.page.current = response.data.pages.current;\r\n                    this.page.all = response.data.pages.all;\r\n                }\r\n            );\r\n        },\r\n        nextPage() {\r\n            this.page.current += 1;\r\n            NewsServer.readALL({\r\n                page: this.page.current\r\n            }).then(\r\n                (response) => {\r\n                    this.news = response.data.result;\r\n                    this.editing.data = this.news[0];\r\n                    this.editing.notification = this.news[0].notification;\r\n                    this.page.current = response.data.pages.current;\r\n                    this.page.all = response.data.pages.all;\r\n                }\r\n            );\r\n        },\r\n        locationPage(num) {\r\n            this.current = num;\r\n            NewsServer.readALL({\r\n                page: num\r\n            }).then(\r\n                (response) => {\r\n                    this.news = response.data.result;\r\n                    this.editing.data = this.news[0];\r\n                    this.editing.notification = this.news[0].notification;\r\n                    this.page.current = response.data.pages.current;\r\n                    this.page.all = response.data.pages.all;\r\n                }\r\n            );\r\n        }\r\n    }\r\n}\r\n</script>\r\n"]}]}