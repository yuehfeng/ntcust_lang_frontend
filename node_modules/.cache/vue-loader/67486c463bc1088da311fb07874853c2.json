       <th scope=\"col\" class=\"text-center\">\r\n                    科系\r\n                </th>\r\n                <th scope=\"col\">標題</th>\r\n                <th scope=\"col\" class=\"text-center\">排名</th>\r\n                <th scope=\"col\" class=\"text-center\">發布日期</th>\r\n                <th scope=\"col\" class=\"text-center\">更新日期</th>\r\n            </tr>\r\n        </thead>\r\n        <tbody>\r\n            <tr v-for=\"(item, index) in honor\" :key=\"index\">\r\n                <td class=\"text-center fs-5\">\r\n                    <b-badge v-if=\"item.department == 41\" pill variant=\"dark\">應用英文系</b-badge>\r\n                    <b-badge v-if=\"item.department == 42\" pill variant=\"dark\">應用日文系</b-badge>\r\n                    <b-badge v-if=\"item.department == 43\" pill variant=\"dark\">應用中文系</b-badge>\r\n                </td>\r\n                <td>{{ item.title }}</td>\r\n                <td class=\"text-center\">\r\n                    <span v-if=\"item.ranking_style == 1\" class=\"badge rounded-pill bg-warning text-dark\">\r\n                        {{ item.ranking }}\r\n                    </span>\r\n                    <span v-if=\"item.ranking_style == 2\" class=\"badge rounded-pill bg-light text-dark\">\r\n                        {{ item.ranking }}\r\n                    </span>\r\n                    <span v-if=\"item.ranking_style == 3\" class=\"badge rounded-pill bg-secondary\">\r\n                        {{ item.ranking }}\r\n                    </span>\r\n                    <span v-if=\"item.ranking_style == 0\" class=\"badge rounded-pill bg-primary\">\r\n                        {{ item.ranking }}\r\n                    </span>\r\n                </td>\r\n                <td class=\"text-center\">{{ moment(item.created_at).format(\"YYYY-MM-DD HH:MM\") }}</td>\r\n                <td class=\"text-center\">{{ moment(item.updated_at).format(\"YYYY-MM-DD HH:MM\") }}</td>\r\n            </tr>\r\n        </tbody>\r\n    </table>\r\n    <nav aria-label=\"Page navigation example\">\r\n        <ul class=\"pagination justify-content-end\">\r\n            <li class=\"page-item disabled\" v-if=\"page.current == 1\">\r\n                <a @click=\"prevPage\" class=\"page-link\">Previous</a>\r\n            </li>\r\n            <li class=\"page-item\" v-if=\"page.current != 1\">\r\n                <a @click=\"prevPage\" class=\"page-link\">Previous</a>\r\n            </li>\r\n            <li class=\"page-item\" v-for=\"index in rows\" :key=\"index\">\r\n                <a v-if=\"page.current == index\" class=\"page-link bg-primary text-light fw-bolder\" @click=\"locationPage(index)\">{{ index }}</a>\r\n                <a v-if=\"page.current != index\" class=\"page-link\" @click=\"locationPage(index)\">{{ index }}</a>\r\n            </li>\r\n            <li class=\"page-item disabled\" v-if=\"page.current == page.all\">\r\n                <a @click=\"nextPage\" class=\"page-link\">Next</a>\r\n            </li>\r\n            <li class=\"page-item\" v-if=\"page.current != page.all\">\r\n                <a @click=\"nextPage\" class=\"page-link\">Next</a>\r\n            </li>\r\n        </ul>\r\n    </nav>\r\n</template>\r\n\r\n<script>\r\nimport moment from 'moment';\r\nimport { HonorServer } from \"../../providers/http-server.js\";\r\n\r\nimport Loading from 'vue-loading-overlay';\r\nimport 'vue-loading-overlay/dist/vue-loading.css';\r\n\r\nexport default {\r\n    name: 'App',\r\n    data() {\r\n        return {\r\n            honor: [],\r\n            page: {\r\n                current: 1,\r\n                all: 1\r\n            },\r\n            query: {\r\n                isLoading: true,\r\n                fullPage: true\r\n            }\r\n        }\r\n    },\r\n    components: {\r\n        Loading\r\n    },\r\n    created() {\r\n        this.moment = moment;\r\n        HonorServer.read().then(\r\n            (response) => {\r\n                this.honor = response.data;\r\n                this.page.all = Math.ceil(response.data.length / 10);\r\n                this.query.isLoading = false;\r\n            }\r\n        );\r\n    },\r\n    computed: {\r\n        rows(){\r\n            return this.page.all\r\n        }\r\n    }\r\n};\r\n</script>\r\n\r\n<style scoped></style>"]}]}