             <span v-if=\"item.ranking_style == 2\" class=\"badge rounded-pill bg-light text-dark\">\r\n                        {{ item.ranking }}\r\n                    </span>\r\n                    <span v-if=\"item.ranking_style == 3\" class=\"badge rounded-pill bg-secondary\">\r\n                        {{ item.ranking }}\r\n                    </span>\r\n                    <span v-if=\"item.ranking_style == 0\" class=\"badge rounded-pill bg-primary\">\r\n                        {{ item.ranking }}\r\n                    </span>\r\n                </td>\r\n                <td class=\"text-center\">{{ moment(item.created_at).format(\"YYYY-MM-DD HH:MM\") }}</td>\r\n                <td class=\"text-center\">{{ moment(item.updated_at).format(\"YYYY-MM-DD HH:MM\") }}</td>\r\n            </tr>\r\n        </tbody>\r\n    </table>\r\n</template>\r\n\r\n<script>\r\nimport moment from 'moment'\r\nimport axios from 'axios';\r\nexport default {\r\n    name: 'App',\r\n    data()\r\n    {\r\n        return {\r\n            honor: []\r\n        }\r\n    },\r\n    components: {\r\n    },\r\n    created()\r\n    {\r\n        this.moment = moment;\r\n        axios\r\n            .get('http://localhost/ntcust_lang_backend/public/api/honor')\r\n            .then(response => {\r\n                this.honor = response.data.data;\r\n            })\r\n            .catch(error => {\r\n                console.error(error)\r\n            });\r\n    }\r\n};\r\n</script>"]}]}