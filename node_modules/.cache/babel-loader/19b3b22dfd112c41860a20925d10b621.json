Q;AAFD,gBAAA,EAAE,EAAC;AAEF,eAFR,EAA0B;kCACtB;AAAA,yBAAiE,CAAjE,YAAA,CAAiE,mBAAjE,EAAiE;AAAvD,oBAAA,OAAO,EAAC,aAA+C;AAAhC,oBAAA,OAAK,EAAE,QAAA,CAAA;AAAyB,mBAAjE,E;sCAAoD;AAAA,6BAAE,C,UAAA,CAAF;AAAA,qB;;;;mBAApD,E;;AAAA,oB,WAAA,CAAiE,CAAjE;AAAA,iB,CADsB;;;;AAAA,eAA1B,CAJQ,CAFR;AAAA,a,CADe;;;;AAAA,WAAnB,CAVQ,CAPR;AAAA,S,CA4BK;;;;AAAA,OA7BT,CA6BS,CA7BT;AAAA,K;;;;GADJ,C","sourcesContent":["<template>\r\n    <b-button v-b-toggle.collapse-1 variant=\"success m-2\">建立消息</b-button>\r\n    <b-alert v-model=\"response.status\" variant=\"success\" dismissible>\r\n        操作成功\r\n    </b-alert>\r\n    <b-alert v-model=\"response.error\" variant=\"danger\" dismissible>\r\n        {{ response.feedback }}\r\n    </b-alert>\r\n    <b-collapse id=\"collapse-1\" class=\"m-2\">\r\n        <b-card>\r\n            <b-row class=\"my-2\">\r\n                <b-col md=\"2\">\r\n                    <label for=\"\">消息標題</label>\r\n                </b-col>\r\n                <b-col md=\"10\">\r\n                    <b-form-input v-model=\"creating.data.title\" placeholder=\"請填入消息標題\"></b-form-input>\r\n                </b-col>\r\n            </b-row>\r\n\r\n            <label for=\"\">內文</label>\r\n            <b-form-textarea\r\n                id=\"textarea\"\r\n                v-model=\"creating.data.context\"\r\n                placeholder=\"請填入消息內文\"\r\n                rows=\"10\"\r\n                max-rows=\"6\"\r\n                ></b-form-textarea>\r\n            <b-row class=\"my-2\">\r\n                <b-col md=\"2\">\r\n                    <label for=\"\">消息類型</label>\r\n                </b-col>\r\n                <b-col md=\"10\">\r\n                    <b-form-select v-model=\"creating.notification.id\" :options=\"options\"></b-form-select>\r\n                </b-col>\r\n                <b-col md=\"12 text-center\">\r\n                    <b-button variant=\"success m-2\" @click=\"createData\">創建</b-button>\r\n                </b-col>\r\n            </b-row>\r\n        </b-card>\r\n    </b-collapse>\r\n</template>\r\n\r\n<script>\r\nimport { NewsServer } from \"../../providers/http-server.js\";\r\n\r\nexport default {\r\n    name: 'App',\r\n    data(){\r\n        return {\r\n            options: [\r\n                { value: 1, text: '公告' },\r\n                { value: 2, text: '研習' },\r\n                { value: 3, text: '活動' },\r\n                { value: 4, text: '競賽' },\r\n                { value: 5, text: '徵才' }\r\n            ],\r\n            creating: {\r\n                data: [],\r\n                notification: {\r\n                    id: 1\r\n                }\r\n            },\r\n            response: {\r\n                status: false,\r\n                feedback: false,\r\n                error: false,\r\n                code: 200\r\n            }\r\n        }\r\n    },\r\n    methods: \r\n    {\r\n        createData(){\r\n            let data = {\r\n                \"title\": this.creating.data.title,\r\n                \"context\": this.creating.data.context,\r\n                \"type\": Number(this.creating.notification.id),\r\n            };\r\n            NewsServer.create(data).then(() => {\r\n                this.response.error = false;\r\n                this.response.status = true;\r\n                this.$router.go(0);\r\n            }).catch(()=> {\r\n                this.response.error = true;\r\n                this.response.status = true;\r\n            });\r\n        }\r\n    }\r\n}\r\n</script>\r\n"],"sourceRoot":""}]}