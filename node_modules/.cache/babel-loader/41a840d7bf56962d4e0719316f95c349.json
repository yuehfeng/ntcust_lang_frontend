: 'post',\r\n                url: 'http://localhost/ntcust_lang_backend/public/api/auth/login',\r\n                data: this.data,\r\n                dataType: 'json',\r\n                headers: {\r\n                    \"Content-Type\": \"application/json\",\r\n                    \"Accept\": \"application/json\"\r\n                }\r\n            })\r\n                .then( response => {\r\n                    console.log(response.data)\r\n                    this.response.feedback = true;\r\n                    this.response.status = response.data.status;\r\n                    localStorage.setItem('API_TOKEN', response.data.parameter);\r\n                })\r\n                .catch( (error) => {\r\n                    this.response.feedback = false;\r\n                    console.log(error)\r\n                })\r\n        }\r\n    }\r\n}\r\n</script>\r\n"],"sourceRoot":""}]}