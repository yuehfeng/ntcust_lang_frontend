b-col md=\"4 text-center\" v-for=\"(item, index) in members\" :key=\"index\">\r\n                <b-card\r\n                    :title=\"item.name + ' ' + item.title\"\r\n                    img-top\r\n                    tag=\"article\"\r\n                    class=\"mb-2\"\r\n                >\r\n                    <b-card-text>\r\n                        <b-avatar src=\"https://placekitten.com/300/300\" size=\"10rem\"></b-avatar>\r\n                        <div class=\"my-3\">\r\n                            <table class=\"table text-start\" v-if=\"item.position != '助教'\">\r\n                                <tbody>\r\n                                    <tr>\r\n                                        <td>職稱</td>\r\n                                        <td>{{ item.position }}</td>\r\n                                    </tr>\r\n                                    <tr>\r\n                                        <td>學歷</td>\r\n                                        <td>{{ item.education }}</td>\r\n                                    </tr>\r\n                                    <tr>\r\n                                        <td>研究領域</td>\r\n                                        <td>{{ item.research }}</td>\r\n                                    </tr>\r\n                                </tbody>\r\n                            </table>\r\n                            <table class=\"table text-start\" v-if=\"item.position == '助教'\">\r\n                                <tbody>\r\n                                    <tr>\r\n                                        <td>職稱</td>\r\n                                        <td>{{ item.position }}</td>\r\n                                    </tr>\r\n                                    <tr>\r\n                                        <td>承辦業務</td>\r\n                                        <td>{{ item.research }}</td>\r\n                                    </tr>\r\n                                </tbody>\r\n                  