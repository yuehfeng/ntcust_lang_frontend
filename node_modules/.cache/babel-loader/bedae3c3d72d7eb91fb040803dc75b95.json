UtB,EAAV,EAAcF,QAAd,EAAwB;AAClC,WAAOR,OAAM,CAAC,4BAA4BU,EAA7B,EAAiCF,QAAjC,CAAb;AACH,GA3BqB;AA4BtByB,EAAAA,YAAY,EAAE,sBAAUvB,EAAV,EAAcF,QAAd,EAAwB;AAClC,WAAOJ,OAAM,CAAC,4BAA4BM,EAA7B,EAAiCF,QAAjC,CAAb;AACH,GA9BqB;AA+BtB0B,EAAAA,YAAY,EAAE,sBAAUxB,EAAV,EAAcF,QAAd,EAAwB;AAClC,WAAON,MAAM,CAAC,4BAA4BQ,EAA7B,EAAiCF,QAAjC,CAAb;AACH;AAjCqB,CAAnB;AAoCP,OAAO,IAAM2B,YAAY,GAAG;AACxBzC,EAAAA,IAAI,EAAE,cAAUc,QAAV,EAAoB;AACtB,WAAOR,OAAM,CAAC,YAAD,EAAeQ,QAAf,CAAb;AACH;AAHuB,CAArB","sourcesContent":["import axios from \"axios\";\r\n\r\n// 伺服器請求時間上限\r\naxios.defaults.timeout = 10000;\r\n// 域名設定\r\naxios.defaults.baseURL = \"http://localhost/ntcust_lang_backend/public/\";\r\n\r\naxios.interceptors.request.use(\r\n    config => {\r\n        config.data = JSON.stringify(config.data);\r\n        // 如果沒有cors的問題則可以都不加\r\n        config.headers = {\r\n            'Content-Type': 'application/json',\r\n            \"Accept\": \"application/json\",\r\n            \"Access-Control-Allow-Origin\": \"*\",\r\n            \"Access-Control-Allow-Methods\": \"GET, PUT, POST, DELETE, OPTIONS\",\r\n            \"Access-Control-Max-Age\": \"86400\",\r\n            \"Authorization\": 'Bearer ' + localStorage.getItem('API_TOKEN'),\r\n        };\r\n        return config;\r\n    },\r\n    error => {\r\n        return Promise.reject(error);\r\n    }\r\n);\r\n\r\n// 異常處理\r\naxios.interceptors.response.use(\r\n    response => {\r\n        // console.clear()\r\n        return response;\r\n    },\r\n    err => {\r\n        // console.clear()\r\n        if (err && err.response) {\r\n            switch (err.response.status) {\r\n                case 400: console.log(\"使用者端請求資料格式錯誤、或者有遺漏\"); break;\r\n                case 401: console.log(\"無效的輸入資料，或資料已存在、已刪除\"); break;\r\n                case 403: console.log(\"使用者之權限不足或已被停權\"); break;\r\n                case 404: console.log(\"找不到該資料或該頁面\"); break;\r\n                case 419: console.log(\"CORS請求問題\"); break;\r\n                case 429: console.log(\"過多請求在同時間(Too Many Requests)\"); break;\r\n                case 500: console.log(\"伺服器出錯，請稍後再試試\"); break;\r\n                case 503: console.log(\"伺服器停止運作\"); break;\r\n            }\r\n        } else {\r\n            console.log(\"連接到服務器失敗\");\r\n        }\r\n        Promise.resolve(err.response);\r\n    }\r\n);\r\n\r\n// 讀取\r\nexport function read(url, params = {}) {\r\n    return new Promise((resolve, reject) => {\r\n        axios\r\n            .get(url, {\r\n                params: params\r\n            })\r\n            .then(response => {\r\n                resolve(response.data);\r\n            })\r\n            .catch(err => {\r\n                reject(err);\r\n            });\r\n    });\r\n}\r\n\r\n// 新增\r\nexport function create(url, data = {}) {\r\n    return new Promise((resolve, reject) => {\r\n        axios.post(url, data).then(\r\n            response => {\r\n                resolve(response.data);\r\n            },\r\n            err => {\r\n                reject(err);\r\n            }\r\n        );\r\n    });\r\n}\r\n\r\n// 移除\r\nexport function remove(url, data = {}) {\r\n    return new Promise((resolve, reject) => {\r\n        axios.delete(url, data).then(\r\n            response => {\r\n                resolve(response.data);\r\n            },\r\n            err => {\r\n                reject(err);\r\n            }\r\n        );\r\n    });\r\n}\r\n\r\n// 更新\r\nexport function update(url, data = {}) {\r\n    return new Promise((resolve, reject) => {\r\n        axios.post(url, data).then(\r\n            response => {\r\n                resolve(response.data);\r\n            },\r\n            err => {\r\n                reject(err);\r\n            }\r\n        );\r\n    });\r\n}\r\n\r\n// 置頂\r\nexport function set(url, data = {}) {\r\n    return new Promise((resolve, reject) => {\r\n        axios.put(url, data).then(\r\n            response => {\r\n                resolve(response.data);\r\n            },\r\n            err => {\r\n                reject(err);\r\n            }\r\n        );\r\n    });\r\n}\r\n\r\n// 將封裝的方法打包起來 命名 NewsServer\r\nexport const NewsServer = {\r\n    read: function (paramObj) {\r\n        return read(\"api/news\", paramObj);\r\n    },\r\n    item: function (id, paramObj) {\r\n        return read(\"api/news/\" + id, paramObj);\r\n    },\r\n    readALL: function (paramObj) {\r\n        return read(\"api/news/all\", paramObj);\r\n    },\r\n    create: function (paramObj) {\r\n        return create(\"api/news/create\", paramObj);\r\n    },\r\n    update: function (id, paramObj) {\r\n        return update(\"api/news/update/\" + id, paramObj);\r\n    },\r\n    top: function (id, paramObj) {\r\n        return set(\"api/news/top/\" + id, paramObj);\r\n    },\r\n    softdelete: function (id, paramObj) {\r\n        return remove(\"api/news/softdelete/\" + id, paramObj);\r\n    },\r\n    delete: function (id, paramObj) {\r\n        return remove(\"api/news/delete/\" + id, paramObj);\r\n    }\r\n};\r\n\r\nexport const BannerServer = {\r\n    read: function (paramObj) {\r\n        return read(\"api/banner\", paramObj);\r\n    },\r\n    create: function (paramObj) {\r\n        return create(\"api/banner/create\", paramObj);\r\n    },\r\n    update: function (id, paramObj) {\r\n        return update(\"api/banner/update/\" + id, paramObj);\r\n    },\r\n    delete: function (id, paramObj) {\r\n        return remove(\"api/banner/delete/\" + id, paramObj);\r\n    }\r\n};\r\n\r\nexport const HonorServer = {\r\n    read: function (paramObj) {\r\n        return read(\"api/honor\", paramObj);\r\n    },\r\n    item: function (id, paramObj) {\r\n        return read(\"api/honor/\" + id, paramObj);\r\n    },\r\n    create: function (paramObj) {\r\n        return create(\"api/honor/create\", paramObj);\r\n    },\r\n    update: function (id, paramObj) {\r\n        return update(\"api/honor/update/\" + id, paramObj);\r\n    },\r\n    delete: function (id, paramObj) {\r\n        return remove(\"api/honor/delete/\" + id, paramObj);\r\n    }\r\n};\r\n\r\nexport const AlbumServer = {\r\n    read: function (paramObj) {\r\n        return read(\"api/album\", paramObj);\r\n    },\r\n    readALL: function (paramObj) {\r\n        return read(\"api/album\", paramObj);\r\n    },\r\n    item: function (id, paramObj) {\r\n        return read(\"api/album/\" + id, paramObj);\r\n    },\r\n    create: function (paramObj) {\r\n        return create(\"api/album/create\", paramObj);\r\n    },\r\n    update: function (id, paramObj) {\r\n        return update(\"api/album/update/\" + id, paramObj);\r\n    },\r\n    delete: function (id, paramObj) {\r\n        return remove(\"api/album/delete/\" + id, paramObj);\r\n    }\r\n};\r\n\r\nexport const FileServer = {\r\n    item: function (id, paramObj) {\r\n        return read(\"api/file/\" + id, paramObj);\r\n    },\r\n    filter: function (type_id, sheet_id, paramObj) {\r\n        return read(\"api/file/filter/\" + type_id + \"/\" + sheet_id, paramObj);\r\n    },\r\n    create: function (paramObj) {\r\n        return create(\"api/file/create\", paramObj);\r\n    },\r\n    update: function (id, paramObj) {\r\n        return update(\"api/file/update/\" + id, paramObj);\r\n    },\r\n    delete: function (id, paramObj) {\r\n        return remove(\"api/file/delete/\" + id, paramObj);\r\n    }\r\n};\r\n\r\n\r\nexport const FileLinkServer = {\r\n    create: function (paramObj) {\r\n        return create(\"api/file/link/create\", paramObj);\r\n    },\r\n    update: function (id, paramObj) {\r\n        return update(\"api/file/link/update/\"+ id, paramObj);\r\n    },\r\n    delete: function (id, paramObj) {\r\n        return remove(\"api/file/link/delete/\" + id, paramObj);\r\n    }\r\n};\r\n\r\nexport const LinkServer = {\r\n    read: function (paramObj) {\r\n        return read(\"api/link\", paramObj);\r\n    },\r\n    item: function (id, paramObj) {\r\n        return read(\"api/link/\" + id, paramObj);\r\n    },\r\n    create: function (paramObj) {\r\n        return create(\"api/link/create\", paramObj);\r\n    },\r\n    update: function (id, paramObj) {\r\n        return update(\"api/link/update/\" + id, paramObj);\r\n    },\r\n    delete: function (id, paramObj) {\r\n        return remove(\"api/link/delete/\" + id, paramObj);\r\n    }\r\n};\r\n\r\nexport const UserServer = {\r\n    Login: function (paramObj) {\r\n        return update(\"api/auth/login\", paramObj);\r\n    },\r\n    Logout: function (paramObj) {\r\n        return update(\"api/auth/logout\", paramObj);\r\n    },\r\n    ReadMember: function (paramObj) {\r\n        return read(\"api/auth/member\", paramObj);\r\n    },\r\n    ReadAccount: function (paramObj) {\r\n        return read(\"api/auth/account\", paramObj);\r\n    },\r\n    Register: function (paramObj) {\r\n        return create(\"api/auth/register\", paramObj);\r\n    },\r\n    UpdateAccount: function (id, paramObj) {\r\n        return update(\"api/auth/update/\" + id, paramObj);\r\n    },\r\n    BanAccount: function (id, paramObj) {\r\n        return set(\"api/auth/ban/\" + id, paramObj);\r\n    },\r\n    DeleteAccount: function (id, paramObj) {\r\n        return remove(\"api/auth/delete/\" + id, paramObj);\r\n    },\r\n    CreateMember: function (id, paramObj) {\r\n        return create(\"api/auth/member/create/\" + id, paramObj);\r\n    },\r\n    UpdateMember: function (id, paramObj) {\r\n        return update(\"api/auth/member/update/\" + id, paramObj);\r\n    },\r\n    DeleteMember: function (id, paramObj) {\r\n        return remove(\"api/auth/member/delete/\" + id, paramObj);\r\n    },\r\n};\r\n\r\nexport const SearchServer = {\r\n    read: function (paramObj) {\r\n        return create(\"api/search\", paramObj);\r\n    }\r\n};"]}]}