    <b-tabs content-class=\"mt-3\">\r\n            <b-tab title=\"相關連結\" active>\r\n                <div v-for=\"(item, index) in link.relevance\" :key=\"index\">\r\n                    <a :href=\"item.url\">{{ item.name }}</a>\r\n                </div>\r\n            </b-tab>\r\n            <b-tab title=\"學生系會\">\r\n                <div v-for=\"(item, index) in link.student_council\" :key=\"index\">\r\n                    <a :href=\"item.url\">{{ item.name }}</a>\r\n                </div>\r\n            </b-tab>\r\n            <b-tab title=\"全國科技大學相關學院\">\r\n                <fieldset>\r\n                    <legend>北部地區</legend>\r\n                    <div v-for=\"(item, index) in link.university.associate_tech_university_north\" :key=\"index\">\r\n                        <a :href=\"item.url\">{{ item.name }}</a>\r\n                    </div>\r\n                </fieldset>\r\n                <fieldset>\r\n                    <legend>北部地區</legend>\r\n                    <div v-for=\"(item, index) in link.university.associate_tech_university_north\" :key=\"index\">\r\n                        <a :href=\"item.url\">{{ item.name }}</a>\r\n                    </div>\r\n                </fieldset>\r\n                <fieldset>\r\n                    <legend>中部地區</legend>\r\n                    <div v-for=\"(item, index) in link.university.associate_tech_university_central\" :key=\"index\">\r\n                        <a :href=\"item.url\">{{ item.name }}</a>\r\n                    </div>\r\n                </fieldset>\r\n                <fieldset>\r\n                    <legend>南部地區</legend>\r\n                    <div v-for=\"(item, index) in link.university.associate_tech_university_south\" :key=\"index\">\r\n                        <a :href=\"item.url\">{{ item.name }}</a>\r\n                    </div>\r\n                </fieldset>\r\n                <fieldset>\r\n                    <legend>離島地區</legend>\r\n                    <div v-for=\"(item, index) in link.university.associate_tech_university_land\" :key=\"index\">\r\n                        <a :href=\"item.url\">{{ item.name }}</a>\r\n                    </div>\r\n                </fieldset>\r\n                \r\n                \r\n                \r\n            </b-tab>\r\n            <b-tab title=\"本校資訊\">\r\n                <div v-for=\"(item, index) in link.information\" :key=\"index\">\r\n                    <a :href=\"item.url\">{{ item.name }}</a>\r\n                </div>\r\n            </b-tab>\r\n        </b-tabs>\r\n    </div>\r\n\r\n</template>\r\n\r\n<script>\r\nimport axios from 'axios';\r\n\r\nexport default {\r\n    name: 'App',\r\n    data(){\r\n        return {\r\n            link: {\r\n                relevance: [],\r\n                student_council: [],\r\n                university: {\r\n                    associate_tech_university_north: [],\r\n                    associate_tech_university_central: [],\r\n                    associate_tech_university_south: [],\r\n                    associate_tech_university_land: []\r\n                },\r\n                information: []\r\n            }\r\n        }\r\n    },\r\n    created(){\r\n        axios\r\n            .get('http://localhost/ntcust_lang_backend/public/api/link')\r\n            .then(response => {\r\n                this.link.relevance = response.data.data.relevance;\r\n                this.link.student_council = response.data.data.student_council;\r\n                this.link.university.associate_tech_university_north = response.data.data.associate_tech_university_north;\r\n                this.link.university.associate_tech_university_central = response.data.data.associate_tech_university_central;\r\n                this.link.university.associate_tech_university_south = response.data.data.associate_tech_university_south;\r\n                this.link.university.associate_tech_university_land = response.data.data.associate_tech_university_land;\r\n                this.link.information = response.data.data.information;\r\n                console.log(response.data.data);\r\n            })\r\n            .catch(error => {\r\n                console.error(error)\r\n            });\r\n    }\r\n}\r\n</script>\r\n"],"sourceRoot":""}]}