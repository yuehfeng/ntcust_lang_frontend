table-hover\">\r\n                <tbody>\r\n                    <tr v-for=\"(item, index) in search.honor.data\" :key=\"index\">\r\n                        <td class=\"text-center\">\r\n                            <router-link :to=\"'/news/' + item.id\">{{item.title}}</router-link>\r\n                        </td>\r\n                    </tr>\r\n                </tbody>\r\n            </table>\r\n            <h2>在連結與檔案中找到{{ search.link.rows }}筆資料</h2>\r\n            <table class=\"table table-hover\">\r\n                <tbody>\r\n                    <tr v-for=\"(item, index) in search.link.data\" :key=\"index\">\r\n                        <td class=\"text-center\">\r\n                            <router-link :to=\"'/link/' + item.id\">{{item.name}}</router-link>\r\n                        </td>\r\n                    </tr>\r\n                </tbody>\r\n            </table>\r\n        </div>\r\n    </div>\r\n    <div class=\"text-center\">\r\n        <button type=\"button\" class=\"btn btn-secondary my-3\" @click=\"historyPrev\">回上頁</button>\r\n    </div>\r\n</template>\r\n\r\n<script>\r\nimport moment from 'moment';\r\nimport { SearchServer } from \"../../providers/http-server.js\";\r\nexport default {\r\n    name: 'App',\r\n    data() {\r\n        return {\r\n            search: {\r\n                news: {\r\n                    data: [],\r\n                    rows: 0\r\n                },\r\n                album: {\r\n                    data: [],\r\n                    rows: 0\r\n                },\r\n                honor: {\r\n                    data: [],\r\n                    rows: 0\r\n                },\r\n                link: {\r\n                    data: [],\r\n                    rows: 0\r\n                },\r\n            },\r\n            keywork: ''\r\n        }\r\n    },\r\n    created(){\r\n        this.moment = moment;\r\n\r\n        this.keyword = this.$route.params.keyword;\r\n        if(this.keyword != '')\r\n            SearchServer.read({keyword: this.keyword}).then(\r\n                (response) => {\r\n                    console.log(response)\r\n                    this.search = response.data;\r\n                }\r\n            );\r\n    },\r\n    updated() {\r\n        this.keyword = this.$route.params.keyword;\r\n        if(this.keyword != '')\r\n            SearchServer.read({keyword: this.keyword}).then(\r\n                (response) => {\r\n                    console.log(response)\r\n                    this.search = response.data;\r\n                }\r\n            );\r\n    },\r\n    methods: {\r\n        historyPrev() {\r\n            this.$router.go(-1);\r\n        }\r\n    }\r\n};\r\n</script>\r\n"],"sourceRoot":""}]}