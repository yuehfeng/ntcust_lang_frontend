Ue,QAAV,EAAoB;AACtB,WAAOf,KAAI,CAAC,YAAD,EAAee,QAAf,CAAX;AACH,GAHsB;AAIvBT,EAAAA,MAAM,EAAE,gBAAUS,QAAV,EAAoB;AACxB,WAAOT,OAAM,CAAC,mBAAD,EAAsBS,QAAtB,CAAb;AACH,GANsB;AAOvBJ,EAAAA,MAAM,EAAE,gBAAUK,EAAV,EAAcD,QAAd,EAAwB;AAC5B,WAAOJ,OAAM,CAAC,uBAAuBK,EAAxB,EAA4BD,QAA5B,CAAb;AACH,GATsB;AAUvBN,EAAAA,MAAM,EAAE,iBAAUO,EAAV,EAAcD,QAAd,EAAwB;AAC5B,WAAOL,MAAM,CAAC,uBAAuBM,EAAxB,EAA4BD,QAA5B,CAAb;AACH;AAZsB,CAApB","sourcesContent":["import axios from \"axios\";\r\n\r\n// 伺服器請求時間上限\r\naxios.defaults.timeout = 10000;\r\n// 域名設定\r\naxios.defaults.baseURL = \"http://localhost/ntcust_lang_backend/public/\";\r\n\r\naxios.interceptors.request.use(\r\n    config => {\r\n        config.data = JSON.stringify(config.data);\r\n        // 如果沒有cors的問題則可以都不加\r\n        config.headers = {\r\n            \"Content-Type\": \"application/json\",\r\n            \"Accept\": \"application/json\",\r\n            \"Access-Control-Allow-Origin\": process.env.API_ROOT,\r\n            \"Access-Control-Allow-Methods\": \"GET, PUT, POST, DELETE, OPTIONS\",\r\n            \"Access-Control-Max-Age\": \"86400\",\r\n            \"Authorization\": 'Bearer ' + localStorage.getItem('API_TOKEN'),\r\n        };\r\n        return config;\r\n    },\r\n    error => {\r\n        return Promise.reject(error);\r\n    }\r\n);\r\n\r\n// 異常處理\r\naxios.interceptors.response.use(\r\n    response => {\r\n        return response;\r\n    },\r\n    err => {\r\n        if (err && err.response) {\r\n            switch (err.response.status) {\r\n                case 404: console.log(\"找不到該頁面\"); break;\r\n                case 500: console.log(\"伺服器出錯\"); break;\r\n                case 503: console.log(\"服務失效\"); break;\r\n                default: console.log(`連接錯誤${err.response.status}`);\r\n            }\r\n        } else {\r\n            console.log(\"連接到服務器失敗\");\r\n        }\r\n        return Promise.resolve(err.response);\r\n    }\r\n);\r\n\r\n// 讀取\r\nexport function read(url, params = {}) {\r\n    return new Promise((resolve, reject) => {\r\n        axios\r\n            .get(url, {\r\n                params: params\r\n            })\r\n            .then(response => {\r\n                resolve(response.data);\r\n            })\r\n            .catch(err => {\r\n                reject(err);\r\n            });\r\n    });\r\n}\r\n\r\n// 新增\r\nexport function create(url, data = {}) {\r\n    return new Promise((resolve, reject) => {\r\n        axios.post(url, data).then(\r\n            response => {\r\n                resolve(response.data);\r\n            },\r\n            err => {\r\n                reject(err);\r\n            }\r\n        );\r\n    });\r\n}\r\n\r\n// 軟刪除=下架\r\nexport function softdelete(url, data = {}) {\r\n    return new Promise((resolve, reject) => {\r\n        axios.delete(url, data).then(\r\n            response => {\r\n                resolve(response.data);\r\n            },\r\n            err => {\r\n                reject(err);\r\n            }\r\n        );\r\n    });\r\n}\r\n\r\n// 移除\r\nexport function remove(url, data = {}) {\r\n    return new Promise((resolve, reject) => {\r\n        axios.delete(url, data).then(\r\n            response => {\r\n                resolve(response.data);\r\n            },\r\n            err => {\r\n                reject(err);\r\n            }\r\n        );\r\n    });\r\n}\r\n\r\n// 更新\r\nexport function update(url, data = {}) {\r\n    return new Promise((resolve, reject) => {\r\n        axios.post(url, data).then(\r\n            response => {\r\n                resolve(response.data);\r\n            },\r\n            err => {\r\n                reject(err);\r\n            }\r\n        );\r\n    });\r\n}\r\n\r\n// 置頂\r\nexport function top(url, data = {}) {\r\n    return new Promise((resolve, reject) => {\r\n        axios.put(url, data).then(\r\n            response => {\r\n                resolve(response.data);\r\n            },\r\n            err => {\r\n                reject(err);\r\n            }\r\n        );\r\n    });\r\n}\r\n\r\n// 將封裝的方法打包起來 命名 NewsServer\r\nexport const NewsServer = {\r\n    read: function (paramObj) {\r\n        return read(\"api/news/all\", paramObj);\r\n    },\r\n    create: function (paramObj) {\r\n        return create(\"api/news/create\", paramObj);\r\n    },\r\n    update: function (id, paramObj) {\r\n        return update(\"api/news/update/\" + id, paramObj);\r\n    },\r\n    top: function (id, paramObj) {\r\n        return top(\"api/news/top/\" + id, paramObj);\r\n    },\r\n    softdelete: function (id, paramObj) {\r\n        return remove(\"api/news/softdelete/\" + id, paramObj);\r\n    },\r\n    delete: function (id, paramObj) {\r\n        return remove(\"api/news/delete/\" + id, paramObj);\r\n    }\r\n};\r\n\r\nexport const BannerServer = {\r\n    read: function (paramObj) {\r\n        return read(\"api/banner\", paramObj);\r\n    },\r\n    create: function (paramObj) {\r\n        return create(\"api/banner/create\", paramObj);\r\n    },\r\n    update: function (id, paramObj) {\r\n        return update(\"api/banner/update/\" + id, paramObj);\r\n    },\r\n    delete: function (id, paramObj) {\r\n        return remove(\"api/banner/delete/\" + id, paramObj);\r\n    }\r\n};\r\n\r\nexport const HonorServer = {\r\n    read: function (paramObj) {\r\n        return read(\"api/banner\", paramObj);\r\n    },\r\n    create: function (paramObj) {\r\n        return create(\"api/banner/create\", paramObj);\r\n    },\r\n    update: function (id, paramObj) {\r\n        return update(\"api/banner/update/\" + id, paramObj);\r\n    },\r\n    delete: function (id, paramObj) {\r\n        return remove(\"api/banner/delete/\" + id, paramObj);\r\n    }\r\n};"]}]}