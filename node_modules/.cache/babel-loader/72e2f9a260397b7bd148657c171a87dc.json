        <div v-html=\"news.context\"></div>\r\n        <div class=\"text-center\">\r\n            <button type=\"button\" class=\"btn btn-secondary my-3\" @click=\"historyPrev\">回上頁</button>\r\n        </div>\r\n    </div>\r\n</template>\r\n\r\n<script>\r\nimport Title from '../components/Title.vue';\r\nimport { NewsServer } from \"../../providers/http-server.js\";\r\nimport moment from 'moment'\r\nlet page = 1;\r\n\r\nexport default {\r\n    name: 'App',\r\n    data() {\r\n        return {\r\n            news: []\r\n        }\r\n    },\r\n    components: { \r\n        Title\r\n    },\r\n    created(){\r\n        this.moment = moment;\r\n        NewsServer.item(page).then(\r\n            (response) => {\r\n                this.news = response.data;\r\n                this.news_type = response.data.notification.type;\r\n            }\r\n        )\r\n    },\r\n    methods: {\r\n        historyPrev() {\r\n            this.$router.go(-1);\r\n        },\r\n        updatePage(page) {\r\n            NewsServer.item(page).then(\r\n            (response) => {\r\n                this.news = response.data;\r\n                this.news_type = response.data.notification.type;\r\n            }\r\n        )\r\n        }\r\n    }\r\n};\r\n</script>\r\n\r\n<style>\r\np{\r\n    text-indent: 2em;\r\n}\r\n</style>\r\n<style>\r\nblockquote{\r\n    color: #545353;\r\n    border-left: 5px solid #9e9e9e;\r\n    padding-left: 5px;\r\n}\r\ntable{\r\n    \r\n}\r\n</style>"],"sourceRoot":""}]}