                 v-model=\"creatinglinks.url\"\r\n                    placeholder=\"請填入網址\"\r\n                    required\r\n                ></b-form-input>\r\n            </b-form-group>\r\n            <b-form-group\r\n                label=\"網址分類\"\r\n                label-for=\"CREATE_LINK_TYPE\"\r\n                invalid-feedback=\"\"\r\n                :state=\"nameState\"\r\n            >\r\n                <b-form-input\r\n                    id=\"CREATE_LINK_TYPE\"\r\n                    v-model=\"creatinglinks.type\"\r\n                    placeholder=\"請填入網址分類\"\r\n                    required>\r\n                </b-form-input>\r\n            </b-form-group>\r\n        </form>\r\n    </b-modal>\r\n    <!-- 編輯用 -->\r\n    <b-modal\r\n        id=\"modal-prevent-closing-4\"\r\n        ref=\"modal-4\"\r\n        title=\"編輯網址資料\"\r\n        @ok=\"updateLink\"\r\n    >\r\n        <form ref=\"form\" @submit.stop.prevent=\"handleSubmit\">\r\n            <b-form-group\r\n                label=\"連結名稱\"\r\n                label-for=\"UPDATE_LINK_NAME\"\r\n                invalid-feedback=\"Name is required\"\r\n            >\r\n                <b-form-input\r\n                    id=\"UPDATE_LINK_NAME\"\r\n                    v-model=\"editinglinks.name\"\r\n                    placeholder=\"請填入連結名稱\"\r\n                    required\r\n                ></b-form-input>\r\n            </b-form-group>\r\n            <b-form-group\r\n                label=\"網址\"\r\n                label-for=\"UPDATE_LINK_URL\"\r\n                invalid-feedback=\"Name is required\"\r\n            >\r\n                <b-form-input\r\n                    id=\"UPDATE_LINK_URL\"\r\n                    v-model=\"editinglinks.url\"\r\n                    placeholder=\"請填入網址\"\r\n                    required\r\n                ></b-form-input>\r\n            </b-form-group>\r\n            <b-form-group\r\n                label=\"網址分類\"\r\n                label-for=\"UPDATE_LINK_TYPE\"\r\n                invalid-feedback=\"\"\r\n            >\r\n                <b-form-input\r\n                    id=\"UPDATE_LINK_TYPE\"\r\n                    v-model=\"editinglinks.type\"\r\n                    placeholder=\"請填入網址分類\"\r\n                    required\r\n                ></b-form-input>\r\n            </b-form-group>\r\n        </form>\r\n    </b-modal>\r\n\r\n    <b-button \r\n    variant=\"success m-1 text-end\"\r\n    v-b-modal.modal-prevent-closing-3>\r\n        建立連結\r\n    </b-button>\r\n    <div>\r\n        <table class=\"table w-100 table-hover\">\r\n            <thead>\r\n                <tr>\r\n                    <th scope=\"col\">#</th>\r\n                    <th scope=\"col\">連結名稱</th>\r\n                    <th scope=\"col\">網站地址</th>\r\n                    <th scope=\"col\">類型</th>\r\n                    <th scope=\"col\">圖標</th>\r\n                    <th scope=\"col\">操作</th>\r\n                </tr>\r\n            </thead>\r\n            <tbody>\r\n                <tr v-for=\"(item, index) in links\" :key=\"item.id\">\r\n                    <th scope=\"row\">{{ index + 1}}</th>\r\n                    <td>{{ item.name }}</td>\r\n                    <td>\r\n                        <a :href=\"item.url\" title=\"\">\r\n                        <b-button variant=\"info m-2\"\r\n                                v-b-popover.hover.bottom=\"item.url\" \r\n                                title=\"查看連結\">\r\n                                連結\r\n                            </b-button>\r\n                        </a>\r\n                    </td>\r\n                    <td>{{ item.type }}</td>\r\n                    <td>{{ item.logo }}</td>\r\n                    <td>\r\n                        <b-button \r\n                        variant=\"success m-1 text-end\"\r\n                        v-b-modal.modal-prevent-closing-4\r\n                        @click=\"edit(index)\">\r\n                            編輯\r\n                        </b-button>\r\n                        <b-button variant=\"danger m-2\">刪除</b-button>\r\n                    </td>\r\n                </tr>\r\n            </tbody>\r\n        </table>\r\n    </div>\r\n</template>\r\n\r\n<script>\r\nimport axios from 'axios';\r\n\r\nexport default {\r\n    name: 'App',\r\n    data(){\r\n        return {\r\n            // Modal\r\n            nameState: false,\r\n            modalShow: false,\r\n            // data\r\n            links: [],\r\n            editinglinks: [],\r\n            creatinglinks: {\r\n                name: '',\r\n                url: '',\r\n                type: '',\r\n                download: false\r\n            }\r\n        }\r\n    },\r\n    created(){\r\n        axios\r\n            .get('http://localhost/ntcust_lang_backend/public/api/link?all=false')\r\n            .then(response => {\r\n                this.links = response.data.data;\r\n            })\r\n            .catch(error => {\r\n                console.error(error)\r\n            });\r\n    },\r\n    computed:{\r\n    },\r\n    methods: \r\n    {\r\n        edit(index)\r\n        {\r\n            this.editinglinks = this.links[index];\r\n        },\r\n        checkFormValidity() {\r\n            const valid = this.$refs.form.checkValidity()\r\n            this.nameState = valid\r\n            return valid\r\n        },\r\n        createLink() {\r\n            axios({\r\n                method: 'post',\r\n                url: 'http://localhost/ntcust_lang_backend/public/api/link/create',\r\n                data: {\r\n                    name: this.creatinglinks.name,\r\n                    url: this.creatinglinks.url,\r\n                    type: this.creatinglinks.type,\r\n                    download: this.creatinglinks.download\r\n                },\r\n                headers: {\r\n                    \"Content-Type\": \"application/json\",\r\n                    \"Accept\": \"application/json\",\r\n                    \"Authorization\": 'Bearer ' + localStorage.getItem('API_TOKEN'),\r\n                }\r\n            })\r\n            .then( () => {\r\n                // console.log(response.data);\r\n            })\r\n            .catch( (error) => {\r\n                console.log(Promise.reject(error));\r\n            });\r\n        },\r\n        updateLink() {\r\n            axios({\r\n                method: 'post',\r\n                url: 'http://localhost/ntcust_lang_backend/public/api/link/update/' + this.editinglinks.id,\r\n                data: this.editinglinks,\r\n                headers: {\r\n                    \"Content-Type\": \"application/json\",\r\n                    \"Accept\": \"application/json\",\r\n                    \"Authorization\": 'Bearer ' + localStorage.getItem('API_TOKEN'),\r\n                }\r\n            })\r\n            .then( () => {\r\n                // console.log(response.data);\r\n            })\r\n            .catch( (error) => {\r\n                console.log(Promise.reject(error));\r\n            });\r\n        },\r\n    }\r\n}\r\n</script>\r\n"],"sourceRoot":""}]}