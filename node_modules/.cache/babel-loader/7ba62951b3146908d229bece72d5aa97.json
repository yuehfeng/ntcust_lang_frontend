   <b-alert v-model=\"response.status\" variant=\"success\" dismissible>\r\n        操作成功\r\n    </b-alert>\r\n    <b-alert v-model=\"response.error\" variant=\"danger\" dismissible>\r\n        {{ response.feedback }}\r\n    </b-alert>\r\n    <b-collapse id=\"collapse-2\" class=\"m-2\">\r\n        <b-card>\r\n            <b-row class=\"my-2\">\r\n                <b-row class=\"my-2\">\r\n                    <b-col md=\"2\">\r\n                        <label for=\"\">得獎名稱</label>\r\n                    </b-col>\r\n                    <b-col md=\"10\">\r\n                        <b-form-input v-model=\"editdata.title\" placeholder=\"請填入得獎名稱\"></b-form-input>\r\n                    </b-col>\r\n                </b-row>\r\n                <b-row class=\"my-2\">\r\n                    <b-col md=\"2\">\r\n                        <label for=\"\">詳細內文</label>\r\n                    </b-col>\r\n                    <b-col md=\"10\">\r\n                        <b-form-textarea\r\n                            id=\"textarea\"\r\n                            v-model=\"editdata.content\"\r\n                            placeholder=\"請填入詳細內文\"\r\n                            rows=\"5\"\r\n                            max-rows=\"5\"\r\n                            >\r\n                        </b-form-textarea>\r\n                    </b-col>\r\n                </b-row>\r\n                <b-row class=\"my-2\">\r\n                    <b-col md=\"1\">\r\n                        <label for=\"\">排名</label>\r\n                    </b-col>\r\n                    <b-col md=\"3\">\r\n                        <b-form-input v-model=\"editdata.ranking\" placeholder=\"請填入得獎名次\"></b-form-input>\r\n                    </b-col>\r\n                    <b-col md=\"1\">\r\n                        <label for=\"\">排名樣式</label>\r\n                    </b-col>\r\n                    <b-col md=\"3\">\r\n                        <b-form-select v-model=\"editdata.ranking_style\" :options=\"options.ranking_styles\"></b-form-select>\r\n                    </b-col>\r\n                    <b-col md=\"1\">\r\n                        <label for=\"\">得獎科系</label>\r\n                    </b-col>\r\n                    <b-col md=\"3\">\r\n                        <b-form-select v-model=\"editdata.department\" :options=\"options.departments\"></b-form-select>\r\n                    </b-col>\r\n                </b-row>\r\n\r\n                <b-col md=\"12 text-center\">\r\n                    <b-button variant=\"success m-2\" @click=\"createData\">創建</b-button>\r\n                </b-col>\r\n            </b-row>\r\n        </b-card>\r\n    </b-collapse>\r\n</template>\r\n\r\n<script>\r\nimport { HonorServer } from '../../providers/http-server';\r\n\r\nexport default {\r\n    name: 'App',\r\n    data(){\r\n        return {\r\n            options: {\r\n                departments: [\r\n                    { value: 41, text: '應用英文系' },\r\n                    { value: 42, text: '應用日文系' },\r\n                    { value: 43, text: '應用中文系' },\r\n                ],\r\n                ranking_styles: [\r\n                    { value: 0, text: '預設樣式' },\r\n                    { value: 1, text: '第一名(金牌)' },\r\n                    { value: 2, text: '第二名(銀牌)' },\r\n                    { value: 3, text: '第三名(銅牌)' },\r\n                ]\r\n            },\r\n            editdata: {\r\n                title: \"\",\r\n                content: \"\",\r\n                ranking: \"\",\r\n                ranking_style: 0,\r\n                department: 41,\r\n            },\r\n            response: {\r\n                status: false,\r\n                feedback: false,\r\n                error: false,\r\n                code: 200\r\n            }\r\n        }\r\n    },\r\n    methods: \r\n    {\r\n        createData(){\r\n            let data = {\r\n                title: this.editdata.title,\r\n                content: this.editdata.content,\r\n                ranking: this.editdata.ranking,\r\n                ranking_style: this.editdata.ranking_style,\r\n                department: this.editdata.department\r\n            };\r\n            HonorServer.create(data).then( (response) => {\r\n                this.response.status = response.status;\r\n                this.response.error = false;\r\n                this.$router.go(0);\r\n            }).catch(() => {\r\n                this.response.status = true;\r\n                this.response.error = true;\r\n            });\r\n        }\r\n    }\r\n}\r\n</script>\r\n"],"sourceRoot":""}]}