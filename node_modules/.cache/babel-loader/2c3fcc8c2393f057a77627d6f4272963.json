</h2>\r\n            <div class=\"text-end\">\r\n                <b-button @click=\"logout\">登出</b-button>\r\n            </div>\r\n            <b-tabs content-class=\"mt-3\" fill>\r\n                <b-tab title=\"消息管理\" active>\r\n                    <Manage-News></Manage-News>\r\n                </b-tab>\r\n                <b-tab title=\"看板管理\">\r\n                    <Manage-Banner></Manage-Banner>\r\n                </b-tab>\r\n                <b-tab title=\"榮譽榜管理\">\r\n                    <ManageHonor></ManageHonor>\r\n                </b-tab>\r\n                <b-tab title=\"相簿管理\">\r\n                    <Manage-Album></Manage-Album>\r\n                </b-tab>\r\n                <b-tab title=\"連結管理\">\r\n                    <Manage-Link></Manage-Link>\r\n                </b-tab>\r\n                <b-tab title=\"成員列表\">\r\n                    <Manage-User></Manage-User>\r\n                </b-tab>\r\n            </b-tabs>\r\n        </div>\r\n    </div>\r\n</template>\r\n\r\n<script>\r\nimport axios from 'axios';\r\nimport ManageNews from '../components/Manage-News.vue';\r\nimport ManageBanner from '../components/Manage-Banner.vue';\r\nimport ManageLink from '../components/Manage-Link.vue';\r\nimport ManageHonor from '../components/Manage-Honor.vue';\r\nimport ManageUser from '../components/Manage-User.vue';\r\nimport ManageAlbum from '../components/Manage-Album.vue';\r\n\r\nexport default {\r\n    components: { \r\n        ManageNews,\r\n        ManageBanner,\r\n        ManageLink,\r\n        ManageHonor,\r\n        ManageUser,\r\n        ManageAlbum\r\n    },\r\n    name: 'App',\r\n    data(){\r\n        return {\r\n            data: {\r\n                email: 's1410931025@gms.nutc.edu.tw',\r\n                password: 'admin1234'\r\n            },\r\n            response: {\r\n                feedback: false,\r\n                status: false,\r\n                cause: ''\r\n            }\r\n        }\r\n    },\r\n    methods: \r\n    {\r\n        login()\r\n        {\r\n            axios({\r\n                method: 'post',\r\n                url: 'http://localhost/ntcust_lang_backend/public/api/auth/login',\r\n                data: this.data,\r\n                dataType: 'json',\r\n                headers: {\r\n                    \"Content-Type\": \"application/json\",\r\n                    \"Accept\": \"application/json\"\r\n                }\r\n            })\r\n                .then( response => {\r\n                    // console.log(response.data)\r\n                    this.response.feedback = true;\r\n                    this.response.status = response.data.status;\r\n                    localStorage.setItem('API_TOKEN', response.data.parameter);\r\n                })\r\n                .catch( (error) => {\r\n                    console.log(Promise.reject(error));\r\n                    this.response.feedback = false;\r\n                    if (error && error.response) {\r\n                        switch (error.response.status) {\r\n                            case 401: console.log(error.response.data.feedback); break;\r\n                            case 404: console.log(\"找不到該頁面\"); break;\r\n                            case 500: console.log(\"伺服器出錯\"); break;\r\n                            case 503: console.log(\"服務失效\"); break;\r\n                            default: console.log(`連接錯誤${error.response.status}`);\r\n                        }\r\n                    } else {\r\n                        console.log(\"連接到服務器失敗\");\r\n                    }\r\n                })\r\n        },\r\n        logout()\r\n        {\r\n            localStorage.removeItem(\"API_TOKEN\");\r\n            axios({\r\n                method: 'post',\r\n                url: 'http://localhost/ntcust_lang_backend/public/api/auth/logout',\r\n                dataType: 'json',\r\n                headers: {\r\n                    \"Content-Type\": \"application/json\",\r\n                    \"Accept\": \"application/json\"\r\n                }\r\n            })\r\n                .then( response => {\r\n                    localStorage.setItem('API_TOKEN', response.data.parameter);\r\n                })\r\n                .catch( (error) => {\r\n                    this.response.feedback = false;\r\n                    if (error && error.response) {\r\n                        switch (error.response.status) {\r\n                            case 404:\r\n                            console.log(\"找不到該頁面\");\r\n                            break;\r\n                            case 500:\r\n                            console.log(\"伺服器出錯\");\r\n                            break;\r\n                            case 503:\r\n                            console.log(\"服務失效\");\r\n                            break;\r\n                            default:\r\n                            console.log(`連接錯誤${error.response.status}`);\r\n                        }\r\n                    } else {\r\n                        console.log(\"連接到服務器失敗\");\r\n                    }\r\n                })\r\n        }\r\n    }\r\n}\r\n</script>\r\n"],"sourceRoot":""}]}