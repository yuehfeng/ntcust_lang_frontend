nsole.log(\"過多請求在同時間(Too Many Requests)\"); break;\r\n                case 500: console.log(\"伺服器出錯\"); break;\r\n                case 503: console.log(\"服務失效\"); break;\r\n                default: console.log(`連接錯誤${err.response.status}`);\r\n            }\r\n        } else {\r\n            console.log(\"連接到服務器失敗\");\r\n        }\r\n        return Promise.resolve(err.response);\r\n    }\r\n);\r\n\r\n// 讀取\r\nexport function read(url, params = {}) {\r\n    return new Promise((resolve, reject) => {\r\n        axios\r\n            .get(url, {\r\n                params: params\r\n            })\r\n            .then(response => {\r\n                resolve(response.data);\r\n            })\r\n            .catch(err => {\r\n                reject(err);\r\n            });\r\n    });\r\n}\r\n\r\n// 新增\r\nexport function create(url, data = {}) {\r\n    return new Promise((resolve, reject) => {\r\n        axios.post(url, data).then(\r\n            response => {\r\n                resolve(response.data);\r\n            },\r\n            err => {\r\n                reject(err);\r\n            }\r\n        );\r\n    });\r\n}\r\n\r\n// 移除\r\nexport function remove(url, data = {}) {\r\n    return new Promise((resolve, reject) => {\r\n        axios.delete(url, data).then(\r\n            response => {\r\n                resolve(response.data);\r\n            },\r\n            err => {\r\n                reject(err);\r\n            }\r\n        );\r\n    });\r\n}\r\n\r\n// 更新\r\nexport function update(url, data = {}) {\r\n    return new Promise((resolve, reject) => {\r\n        axios.post(url, data).then(\r\n            response => {\r\n                resolve(response.data);\r\n            },\r\n            err => {\r\n                reject(err);\r\n            }\r\n        );\r\n    });\r\n}\r\n\r\n// Login\r\nexport function sign(url, data = {}) {\r\n    return new Promise((resolve, reject) => {\r\n        axios.post(url, data).then(\r\n            response => {\r\n                resolve(response.data);\r\n            },\r\n            err => {\r\n                reject(err);\r\n            }\r\n        );\r\n    });\r\n}\r\n\r\n// 推送資料\r\nexport function set(url, data = {}) {\r\n    return new Promise((resolve, reject) => {\r\n        axios.put(url, data).then(\r\n            response => {\r\n                resolve(response.data);\r\n            },\r\n            err => {\r\n                reject(err);\r\n            }\r\n        );\r\n    });\r\n}\r\n\r\n// 將封裝的方法打包起來 命名 UserServer\r\nexport const UserServer = {\r\n    Login: function (paramObj) {\r\n        return sign(\"api/auth/login\", paramObj);\r\n    },\r\n    Logout: function (paramObj) {\r\n        return sign(\"api/auth/logout\", paramObj);\r\n    },\r\n    ReadMember: function (paramObj) {\r\n        return read(\"api/auth/member\", paramObj);\r\n    },\r\n    ReadAccount: function (paramObj) {\r\n        return read(\"api/auth/member\", paramObj);\r\n    },\r\n    Register: function (paramObj) {\r\n        return create(\"api/auth/register\", paramObj);\r\n    },\r\n    UpdateAccount: function (id, paramObj) {\r\n        return update(\"api/auth/update/\" + id, paramObj);\r\n    },\r\n    BanAccount: function (id, paramObj) {\r\n        return set(\"api/auth/ban/\" + id, paramObj);\r\n    },\r\n    DeleteAccount: function (id, paramObj) {\r\n        return remove(\"api/auth/delete/\" + id, paramObj);\r\n    },\r\n    CreateMember: function (id, paramObj) {\r\n        return create(\"api/auth/member/create/\" + id, paramObj);\r\n    },\r\n    UpdateMember: function (id, paramObj) {\r\n        return update(\"api/auth/member/update/\" + id, paramObj);\r\n    },\r\n    DeleteMember: function (id, paramObj) {\r\n        return remove(\"api/auth/member/delete/\" + id, paramObj);\r\n    },\r\n};\r\n"]}]}