d\">\r\n                        <b-button variant=\"success m-1\" >更新</b-button>\r\n                        <b-button variant=\"danger m-1\">刪除</b-button>\r\n                    </td>\r\n                </tr>\r\n                <tr>\r\n                    <td colspan=\"4\">{{ item.content }}</td>\r\n                </tr>\r\n                <tr>\r\n                    <td>{{ item.ranking }}</td>\r\n                    <td>{{ item.department }}</td>\r\n                    <td>{{ item.file_1 }}</td>\r\n                    <td>{{ item.file_2 }}</td>\r\n                </tr>\r\n                <tr>\r\n                    <td>建立時間</td>\r\n                    <td>{{ item.created_at}}</td>\r\n                    <td>更新時間</td>\r\n                    <td>{{ item.updated_at }}</td>\r\n                </tr>\r\n            </tbody>\r\n        </table>\r\n    </div>\r\n</template>\r\n\r\n<script>\r\nimport axios from 'axios';\r\nimport mana\r\n\r\nexport default {\r\n    name: 'App',\r\n    components: {\r\n        ManageBannerCreate\r\n    },\r\n    data(){\r\n        return {\r\n            honor: []\r\n        }\r\n    },\r\n    created(){\r\n        axios\r\n            .get('http://localhost/ntcust_lang_backend/public/api/honor')\r\n            .then(response => {\r\n                this.honor = response.data.data;\r\n            })\r\n            .catch(error => {\r\n                console.error(error)\r\n            });\r\n    },\r\n    computed:{\r\n    },\r\n    methods: \r\n    {\r\n    }\r\n}\r\n</script>\r\n"],"sourceRoot":""}]}