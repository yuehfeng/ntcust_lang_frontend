                     </table>\r\n                            <b-row>\r\n                                <b-col md=\"12\" class=\"text-end\">\r\n                                    <b-button variant=\"success m-1 text-end\">修改</b-button>\r\n                                    <b-button variant=\"danger m-1\">刪除</b-button>\r\n                                </b-col>\r\n                            </b-row>\r\n                        </b-card-text>\r\n                    </b-card-body>\r\n                </b-col>\r\n            </b-row>\r\n        </b-card>\r\n    </div>\r\n</template>\r\n\r\n<script>\r\nimport ManageBannerCreate from './Manage-Banner-Create.vue';\r\nimport moment from 'moment';\r\nimport axios from 'axios';\r\n\r\nexport default {\r\n    name: 'App',\r\n    components: {\r\n        ManageBannerCreate\r\n    },\r\n    data(){\r\n        return {\r\n            banners: []\r\n        }\r\n    },\r\n    created(){\r\n        this.moment = moment;\r\n        axios\r\n            .get('http://localhost/ntcust_lang_backend/public/api/banner')\r\n            .then(response => {\r\n                this.banners = response.data.data;\r\n            })\r\n            .catch(error => {\r\n                console.error(error)\r\n            });\r\n    },\r\n    computed:{\r\n    },\r\n    methods: \r\n    {\r\n    }\r\n}\r\n</script>\r\n"],"sourceRoot":""}]}