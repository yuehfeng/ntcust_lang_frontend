                                   <td>{{ item.position }}</td>\r\n                                    </tr>\r\n                                    <tr>\r\n                                        <td>學歷</td>\r\n                                        <td>{{ item.education }}</td>\r\n                                    </tr>\r\n                                    <tr>\r\n                                        <td>研究領域</td>\r\n                                        <td>{{ item.research }}</td>\r\n                                    </tr>\r\n                                </tbody>\r\n                            </table>\r\n                            <table class=\"table text-start\" v-if=\"item.position == '助教'\">\r\n                                <tbody>\r\n                                    <tr>\r\n                                        <td class=\"fix_width\">職稱</td>\r\n                                        <td>{{ item.position }}</td>\r\n                                    </tr>\r\n                                    <tr>\r\n                                        <td>承辦業務</td>\r\n                                        <td>{{ item.research }}</td>\r\n                                    </tr>\r\n                                </tbody>\r\n                            </table>\r\n                            <table class=\"table text-start\" v-if=\"item.position != '助教'\">\r\n                                <tbody>\r\n                                    <tr>\r\n                                        <td>Email</td>\r\n                                        <td>{{ item.email }}</td>\r\n                                    </tr>\r\n                                    <tr>\r\n                                        <td>TEL</td>\r\n                                        <td>{{ item.tel }}</td>\r\n                                    </tr>\r\n                                    <tr>\r\n                                        <td>Office</td>\r\n                                        <td>{{ item.office }}</td>\r\n                                    </tr>\r\n                                </tbody>\r\n                            </table>\r\n                        </div>\r\n                    </b-card-text>\r\n                </b-card>\r\n            </b-col>\r\n        </b-row>\r\n    </div>\r\n    <div class=\"text-center\">\r\n        <button type=\"button\" class=\"btn btn-secondary my-3\" @click=\"historyPrev\">回上頁</button>\r\n    </div>\r\n</template>\r\n\r\n<script>\r\nimport Title from '../components/Title.vue';\r\nimport moment from 'moment'\r\nimport { UserServer } from '../../providers/http-server';\r\n\r\nexport default {\r\n    name: 'App',\r\n    data(){\r\n        return {\r\n            members: []\r\n        }\r\n    },\r\n    components: { \r\n        Title\r\n    },\r\n    created() {\r\n        this.moment = moment;\r\n        UserServer.ReadMember().then(\r\n            (response) => this.members = response.data\r\n        );        \r\n    },\r\n    methods: {\r\n        historyPrev() {\r\n            this.$router.go(-1);\r\n        }\r\n    }\r\n};\r\n</script>\r\n\r\n<style>\r\n.fix_width{\r\n    width: 60px;\r\n}\r\n</style>"],"sourceRoot":""}]}